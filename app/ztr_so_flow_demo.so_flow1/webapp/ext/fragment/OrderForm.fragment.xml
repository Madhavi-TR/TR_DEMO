<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:l="sap.ui.layout"
	xmlns:mvc="sap.ui.core.mvc">
    <Dialog id="orderDialog" title="Order Details" class="sapUiResponsivePadding--header sapUiResponsivePadding--content sapUiResponsivePadding--footer">
        <content>
            <VBox id="ID0">
                <Label id="ID1" text="Order ID" />
                <Text id="ID2" text="{Order_Id}" />
                <Label id="ID3" text="Order Status" />
                <Text id="ID4" text="{Order_Status}" />
            </VBox>
        </content>
        <beginButton>
            <Button id="ID5" text="Close" press="handler.closebutton" core:require="{ handler: 'ztrsoflowdemo/soflow1/ext/fragment/Process_Flow'}"/>
        </beginButton>
    </Dialog>
</core:FragmentDefinition>